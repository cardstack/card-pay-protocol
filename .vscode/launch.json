  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Tests",
      "cwd": "${workspaceRoot}",
      "program": "${workspaceFolder}/node_modules/.bin/truffle",
      "console": "integratedTerminal",
      "env": {},
      "args": ["test"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Migration",
      "cwd": "${workspaceRoot}",
      "program": "${workspaceFolder}/node_modules/.bin/truffle",
      "console": "integratedTerminal",
      "env": {},
      "args": ["migrate", "--network", "sokol", "--compile-all", "--reset"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run truffle exec",
      "cwd": "${workspaceRoot}",
      "program": "${workspaceFolder}/node_modules/.bin/truffle",
      "console": "integratedTerminal",
      "env": {},
      "args": ["exec", "./lib/default-account.js", "--network", "sokol"]
    }
  ]
}
