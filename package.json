{
  "name": "card-protocol-xdai",
  "version": "0.8.8",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/hardhat compile",
    "build:clean": "./node_modules/.bin/hardhat clean && yarn build",
    "test": "yarn lint && yarn test:sol",
    "test:parallel": "yarn lint && ./node_modules/.bin/mocha",
    "test:sol": "./node_modules/.bin/hardhat test",
    "test:coverage": "./node_modules/.bin/hardhat coverage",
    "test:size": "{ ./node_modules/.bin/hardhat size-contracts | tee /dev/fd/3 | grep -q 'exceed the size limit for mainnet deployment' && exit 1; } 3>&1 || echo 'All contracts within size limit'",
    "test:migration": "env DO_NOT_SET_THIS_ENV_VAR____IS_HARDHAT_CI=true yarn test:migration:sokol && yarn test:migration:xdai",
    "test:migration:sokol": "env HARDHAT_FORKING=sokol FORK_BLOCK_NUMBER=$SOKOL_FORK_BLOCK_NUMBER DEBUG=card-protocol.migration* yarn test:sol",
    "test:migration:sokol:debug": "env HARDHAT_FORKING=sokol npx --node-options='--inspect-brk' hardhat test",
    "test:migration:xdai": "env HARDHAT_FORKING=xdai FORK_BLOCK_NUMBER=$XDAI_FORK_BLOCK_NUMBER DEBUG=card-protocol.migration* yarn test:sol",
    "watch:compile": "./node_modules/.bin/hardhat watch compile",
    "watch:test": "./node_modules/.bin/hardhat watch test",
    "write:abi": "node ./scripts/write-abi.js",
    "lint": "yarn lint:sol && yarn lint:js",
    "lint:js": "./node_modules/.bin/eslint .",
    "lint:sol": "./node_modules/.bin/solhint ./contracts/\\*\\*/\\*.sol -w 0",
    "deploy:contracts": "./node_modules/.bin/hardhat run scripts/deploy/001_initialize_contracts.js",
    "deploy:config-only": "yarn deploy:config:protocol",
    "deploy:config-testnet-only": "yarn deploy:config:manual-feeds && yarn deploy:config:protocol",
    "deploy:config:manual-feeds": "./node_modules/.bin/hardhat run scripts/deploy/002_configure_manual_feeds.js",
    "deploy:config:protocol": "./node_modules/.bin/hardhat run scripts/deploy/004_configure_card_protocol.ts",
    "deploy:config:version": "./node_modules/.bin/hardhat run scripts/deploy/005_set_protocol_version.ts",
    "deploy": "yarn build:clean && yarn deploy:contracts && yarn deploy:config-only",
    "deploy:sokol": "env HARDHAT_NETWORK=sokol yarn deploy",
    "deploy:sokol:skip-verify": "env SKIP_VERIFY=true yarn deploy:sokol",
    "deploy:sokol:upgrade-only": "yarn build:clean && env HARDHAT_NETWORK=sokol yarn deploy:contracts",
    "deploy:sokol:config-only": "env HARDHAT_NETWORK=sokol yarn deploy:config-testnet-only",
    "deploy:sokol:upgrade-only:skip-verify": "env SKIP_VERIFY=true yarn deploy:sokol:upgrade-only",
    "deploy:sokol:version": "env HARDHAT_NETWORK=sokol yarn deploy:config:version",
    "deploy:xdai": "env HARDHAT_NETWORK=xdai yarn deploy",
    "deploy:xdai:skip-verify": "env SKIP_VERIFY=true yarn deploy:xdai",
    "deploy:xdai:upgrade-only": "yarn build:clean && env HARDHAT_NETWORK=xdai yarn deploy:contracts",
    "deploy:xdai:config-only": "env HARDHAT_NETWORK=xdai yarn deploy:config-only",
    "deploy:xdai:upgrade-only:skip-verify": "env SKIP_VERIFY=true yarn deploy:xdai:upgrade-only",
    "deploy:xdai:version": "env HARDHAT_NETWORK=xdai yarn deploy:config:version",
    "deploy:localhost": "yarn deploy:localhost:contracts && yarn deploy:localhost:config:manual-feeds && yarn deploy:localhost:config-only && yarn deploy:localhost:config:version",
    "deploy:localhost:contracts": "env HARDHAT_NETWORK=localhost SKIP_VERIFY=true yarn deploy:contracts",
    "deploy:localhost:config-only": "env HARDHAT_NETWORK=localhost yarn deploy:config-only",
    "deploy:localhost:config:manual-feeds": "env HARDHAT_NETWORK=localhost yarn deploy:config:manual-feeds",
    "deploy:localhost:config:version": "env HARDHAT_NETWORK=localhost yarn deploy:config:version",
    "deploy:smoketest": "yarn deploy:smoketest:upgrade && env BRIDGE_MEDIATOR=0xd1e46b8a371AF04Ce74c993B325b844ef0f48b16 RELAY_SERVER_TX_SENDER=0x76E2cFc1F5Fa8F6a5b3fC4c8F4788F0116861F9B yarn deploy:smoketest:config",
    "deploy:smoketest:upgrade": "rm -f .openzeppelin/addresses-localhost.json .openzeppelin/unknown-31337.json && yarn deploy:localhost:contracts && yarn deploy:localhost:contracts",
    "deploy:smoketest:config": "rm -f .openzeppelin/addresses-localhost.json .openzeppelin/unknown-31337.json && yarn deploy:localhost",
    "deploy:fork": "env HARDHAT_NETWORK=localhost DO_NOT_SET_THIS_ENV_VAR____IS_HARDHAT_CI=true yarn deploy:contracts",
    "deploy:fork:sokol": "env HARDHAT_FORKING=sokol FORK_BLOCK_NUMBER=$SOKOL_FORK_BLOCK_NUMBER yarn deploy:fork",
    "deploy:fork:xdai": "env HARDHAT_FORKING=xdai FORK_BLOCK_NUMBER=$XDAI_FORK_BLOCK_NUMBER yarn deploy:fork",
    "migrate:fork:sokol": "env HARDHAT_FORKING=sokol FORK_BLOCK_NUMBER=$SOKOL_FORK_BLOCK_NUMBER yarn migrate:localhost",
    "migrate:sokol": "env HARDHAT_NETWORK=sokol yarn migrate",
    "migrate:xdai": "env HARDHAT_NETWORK=xdai yarn migrate",
    "migrate:fork:xdai": "env HARDHAT_FORKING=xdai FORK_BLOCK_NUMBER=$XDAI_FORK_BLOCK_NUMBER yarn migrate:localhost",
    "migrate:localhost": "env HARDHAT_NETWORK=localhost DO_NOT_SET_THIS_ENV_VAR____IS_HARDHAT_CI=true yarn migrate",
    "migrate": "env DEBUG=card-protocol.migration* ./node_modules/.bin/hardhat run scripts/migration/migrate_gnosis_1.3.ts",
    "start-forked-node": "./node_modules/.bin/hardhat node --fork $FORK_RPC_URL --fork-block-number $FORK_BLOCK_NUMBER",
    "start-forked-node:sokol": "env FORK_BLOCK_NUMBER=$SOKOL_FORK_BLOCK_NUMBER FORK_RPC_URL=https://sokol.poa.network HARDHAT_FORKING=sokol yarn start-forked-node",
    "start-forked-node:xdai": "env FORK_BLOCK_NUMBER=$XDAI_FORK_BLOCK_NUMBER FORK_RPC_URL=https://rpc.xdaichain.com HARDHAT_FORKING=xdai yarn start-forked-node",
    "console:localhost": "env HARDHAT_NETWORK=localhost DO_NOT_SET_THIS_ENV_VAR____IS_HARDHAT_CI=true npx hardhat console",
    "console:fork:sokol": "env HARDHAT_FORKING=sokol FORK_BLOCK_NUMBER=$SOKOL_FORK_BLOCK_NUMBER yarn console:localhost",
    "verify": "./node_modules/.bin/hardhat run scripts/verify.js",
    "verify:sokol": "env HARDHAT_NETWORK=sokol yarn verify",
    "verify:xdai": "env HARDHAT_NETWORK=xdai yarn verify",
    "version:sokol:patch": "env CARDPAY_VERSION=patch yarn deploy:sokol:version",
    "version:sokol:minor": "env CARDPAY_VERSION=minor yarn deploy:sokol:version",
    "version:sokol:major": "env CARDPAY_VERSION=major yarn deploy:sokol:version",
    "version:xdai:promote": "env CARDPAY_VERSION=promote yarn deploy:xdai:version",
    "release:sokol:patch": "yarn deploy:sokol && yarn version:sokol:patch && node ./lib/release.js sokol patch",
    "release:sokol:patch:skip-verify": "yarn deploy:sokol:skip-verify && yarn version:sokol:patch && node ./lib/release.js sokol patch",
    "release:sokol:patch:config-only": "yarn deploy:sokol:config-only && yarn version:sokol:patch && node ./lib/release.js sokol patch",
    "release:sokol:patch:upgrade-only": "yarn deploy:sokol:upgrade-only && yarn version:sokol:patch && node ./lib/release.js sokol patch",
    "release:sokol:patch:upgrade-only:skip-verify": "yarn deploy:sokol:upgrade-only:skip-verify && yarn version:sokol:patch && node ./lib/release.js sokol patch",
    "release:sokol:minor": "yarn deploy:sokol && yarn version:sokol:minor && node ./lib/release.js sokol minor",
    "release:sokol:minor:config-only": "yarn deploy:sokol:config-only && yarn version:sokol:minor && node ./lib/release.js sokol minor",
    "release:sokol:major": "yarn deploy:sokol && yarn version:sokol:major && node ./lib/release.js sokol major",
    "promote:xdai": "yarn deploy:xdai && yarn version:xdai:promote && node ./lib/release.js xdai promote",
    "promote:xdai:skip-verify": "yarn deploy:xdai:skip-verify && yarn version:xdai:promote && node ./lib/release.js xdai promote",
    "promote:xdai:config-only": "yarn deploy:xdai:config-only && yarn version:xdai:promote && node ./lib/release.js xdai promote",
    "promote:xdai:upgrade-only": "yarn deploy:xdai:upgrade-only && yarn version:xdai:promote && node ./lib/release.js xdai promote",
    "promote:xdai:upgrade-only:skip-verify": "yarn deploy:xdai:upgrade-only:skip-verify && yarn version:xdai:promote && node ./lib/release.js xdai promote"
  },
