{
  "name": "card-protocol-xdai",
  "version": "0.4.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/truffle compile",
    "build:clean": "rm -rf ./build && yarn build",
    "test": "yarn lint && yarn test:sol",
    "test:sol": "./node_modules/.bin/truffle test",
    "test:coverage": "./node_modules/.bin/truffle run coverage",
    "lint": "npm-run-all lint:*",
    "lint:js": "./node_modules/.bin/eslint .",
    "lint:sol": "./node_modules/.bin/solhint ./contracts/**/*.sol -w 0",
    "deploy:sokol": "rm -rf ./build && ./node_modules/.bin/truffle migrate --network sokol --compile-all --reset",
    "deploy:sokol:upgrade-only": "rm -rf ./build && ./node_modules/.bin/truffle migrate --network sokol --compile-all --f 1 --to 1",
    "deploy:xdai": "rm -rf ./build && ./node_modules/.bin/truffle migrate --network xdai --compile-all --reset",
    "deploy:xdai:upgrade-only": "rm -rf ./build && ./node_modules/.bin/truffle migrate --network xdai --compile-all --f 1 --to 1",
    "release-sokol:patch:upgrade-only": "node ./lib/prerelease.js patch && yarn deploy:sokol:upgrade-only && node ./lib/release.js sokol patch",
    "release-sokol:patch": "node ./lib/prerelease.js patch && yarn deploy:sokol && node ./lib/release.js sokol patch",
    "release-sokol:minor": "node ./lib/prerelease.js minor && yarn deploy:sokol && node ./lib/release.js sokol minor",
    "release-sokol:major": "node ./lib/prerelease.js major && yarn deploy:sokol && node ./lib/release.js sokol major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cardstack/card-protocol-xdai.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cardstack/card-protocol-xdai/issues"
  },
  "engines": {
    "node": ">=14.0"
  },
  "homepage": "https://github.com/cardstack/card-protocol-xdai#readme",
  "devDependencies": {
    "@chainlink/contracts": "^0.1.7",
    "@gnosis.pm/mock-contract": "github:gnosis/mock-contract#sol_0_5_0",
    "@gnosis.pm/safe-contracts": "^1.2.0",
    "@openzeppelin/cli": "^2.8.2",
    "@openzeppelin/contract-upgradeable": "OpenZeppelin/openzeppelin-contracts-upgradeable#v2.5.0",
    "@openzeppelin/truffle-upgrades": "^1.7.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bn": "^0.2.1",
    "chai-exclude": "^2.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eth-gas-reporter": "^0.2.19",
    "eth-lightwallet": "^4.0.0",
    "fs-extra": "^10.0.0",
    "ganache-cli": "^6.12.1",
    "glob": "^7.1.6",
    "lodash": "^4.17.21",
    "mocha": "^8.2.1",
    "node-fetch": "^2.6.1",
    "node-fs-extra": "^0.8.2",
    "npm-run-all": "^4.1.5",
    "pify": "^5.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.7",
    "semver": "^7.3.5",
    "solc": "0.5.17",
    "solhint": "^3.3.4",
    "solidity-coverage": "^0.7.11",
    "trezor-cli-wallet-provider": "^1.0.5",
    "truffle": "^5.1.51",
    "truffle-source-verify": "^0.0.6",
    "web3-eth-abi": "^1.3.0",
    "web3-utils": "^1.3.0"
  },
  "resolutions": {
    "bitcore-lib": "8.1.1"
  },
  "volta": {
    "node": "14.16.1"
  }
}